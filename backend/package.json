{
  "name": "englishbrain-backend",
  "version": "0.1.0",
  "description": "Firebase Cloud Functions for English Brain backend",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:contract": "jest --runTestsByPath tests/users.contract.test.ts tests/levelTests.contract.test.ts --runInBand",
    "test:watch": "jest --watch",
    "clean": "rm -rf lib",
    "prepare": "npm run build",
    "dev": "npx firebase emulators:start",
    "firebase:login": "npx firebase login",
    "firebase:use:dev": "npx firebase use development",
    "firebase:use:staging": "npx firebase use staging",
    "firebase:use:prod": "npx firebase use production",
    "deploy": "npx firebase deploy --only functions"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@firebase/rules-unit-testing": "^2.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "firebase-functions-test": "^3.0.0",
    "firebase-tools": "^13.13.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
